function showServiceDetails() {
   var type = $("#service").val();
   if(type == "employee"){
     $("#serviceDetails").show();
     $("#businessDetails").hide();
   } else {
     $("#businessDetails").show();
     $("#serviceDetails").hide();
   }
   
}

function showAstroProfile() {
